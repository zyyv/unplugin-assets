{
  "name": "@unplugin-assets/monorepo",
  "type": "module",
  "version": "0.0.6",
  "private": true,
  "packageManager": "pnpm@8.14.0",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "play": "nr -C playground dev",
    "play:nuxt": "nr -C playground-nuxt dev",
    "prepublishOnly": "nr build",
    "release": "bumpp -r && pnpm -r publish --access public",
    "test": "vitest",
    "test:update": "vitest -u",
    "build": "nr build:core && nr build:client",
    "build:core": "nr -C packages/core build",
    "build:client": "nr -C packages/client build",
    "dev:client": "nr -C packages/client serve",
    "dev": "nr -C packages/core dev"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@antfu/ni": "^0.21.12",
    "@nuxt/kit": "^3.9.0",
    "@nuxt/schema": "^3.9.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "bumpp": "^9.2.1",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "get-port": "^7.0.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unocss-preset-useful": "^0.3.7",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.1",
    "vue": "^3.4.3"
  }
}
