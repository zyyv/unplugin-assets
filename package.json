{
  "name": "unplugin-assets",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.12.0",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/zyyv/unplugin-assets#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyyv/unplugin-assets.git"
  },
  "bugs": {
    "url": "https://github.com/zyyv/unplugin-assets/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "play:nuxt": "npm -C playground-nuxt run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "build": "tsup && vite build ./src/client",
    "dev": "DEV=true tsup --watch",
    "client:dev": "vite ./src/client"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@vueuse/core": "^10.7.1",
    "sirv": "^2.0.4",
    "unplugin": "^1.6.0",
    "vue": "^3.4.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@nuxt/kit": "^3.9.0",
    "@nuxt/schema": "^3.9.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "bumpp": "^9.2.1",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "get-port": "^7.0.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unocss-preset-useful": "^0.3.7",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.1",
    "webpack": "^5.89.0"
  }
}
