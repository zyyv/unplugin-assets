{
  "name": "@unplugin-assets/monorepo",
  "type": "module",
  "version": "0.1.4",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "play": "nr -C playground dev",
    "play:nuxt": "nr -C playground-nuxt dev",
    "prepublishOnly": "nr build",
    "release": "bumpp -r && pnpm -r publish --access public",
    "test": "vitest",
    "test:update": "vitest -u",
    "build": "nr build:core && nr build:client",
    "build:core": "nr -C packages/core build",
    "build:client": "nr -C packages/client build",
    "dev:client": "nr -C packages/client serve",
    "dev": "nr -C packages/core dev"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@antfu/ni": "^0.23.1",
    "@nuxt/kit": "^3.14.159",
    "@nuxt/schema": "^3.14.159",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "bumpp": "^9.8.1",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.7",
    "eslint": "^9.16.0",
    "esno": "^4.8.0",
    "fast-glob": "^3.3.2",
    "get-port": "^7.1.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.28.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "unocss": "^0.65.1",
    "unocss-preset-useful": "^0.9.1",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.3",
    "vitest": "^2.1.8",
    "vue": "^3.5.13"
  }
}
