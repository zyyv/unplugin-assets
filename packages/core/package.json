{
  "name": "unplugin-assets",
  "type": "module",
  "version": "0.1.2",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/zyyv/unplugin-assets#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyyv/unplugin-assets.git"
  },
  "bugs": {
    "url": "https://github.com/zyyv/unplugin-assets/issues"
  },
  "keywords": [
    "unplugin",
    "vite"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:fix": "esno scripts/postbuild.ts",
    "build": "tsup --env.NODE_ENV production",
    "dev": "DEV=true tsup --watch --env.NODE_ENV development"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "image-meta": "^0.2.0",
    "launch-editor": "^2.6.1",
    "open": "^10.1.0",
    "picocolors": "^1.0.1",
    "sirv": "^2.0.4",
    "unplugin": "^1.10.1",
    "vite-dev-rpc": "^0.1.4"
  },
  "devDependencies": {
    "pathe": "^1.1.2"
  }
}
