{
  "name": "unplugin-assets",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.12.0",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/zyyv/unplugin-assets#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyyv/unplugin-assets.git"
  },
  "bugs": {
    "url": "https://github.com/zyyv/unplugin-assets/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:fix": "esno scripts/postbuild.ts",
    "build": "tsup",
    "dev": "DEV=true tsup --watch"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "sirv": "^2.0.4",
    "unplugin": "^1.6.0",
    "vite-dev-rpc": "^0.1.4",
    "fast-glob": "^3.3.2"
  },
  "devDependencies": {
    "pathe": "^1.1.1"
  }
}
